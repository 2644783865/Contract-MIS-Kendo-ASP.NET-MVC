<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @* Kontent CSS *@
    
    <link type="text/css" rel="stylesheet" href="~/Content/themes/default/css/uniform.default.css">
    <link type="text/css" rel="stylesheet" href="~/Content/jquery-ui.css">
    <link type="text/css" rel="stylesheet" href="~/Content/bootstrap.css">
    <link type="text/css" rel="stylesheet" href="~/Content/admin-theme.css">
    <link type="text/css" rel="stylesheet" href="~/Content/fontawesome/css/font-awesome.css">

 
    <link href="http://cdn.kendostatic.com/2014.3.1411/styles/kendo.common-bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.kendostatic.com/2014.3.1411/styles/kendo.mobile.all.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.kendostatic.com/2014.3.1411/styles/kendo.dataviz.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.kendostatic.com/2014.3.1411/styles/kendo.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.kendostatic.com/2014.3.1411/styles/kendo.dataviz.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="http://cdn.kendostatic.com/2014.3.1411/js/jquery.min.js"></script>
    <script src="http://cdn.kendostatic.com/2014.3.1411/js/jszip.min.js"></script>
    <script src="http://cdn.kendostatic.com/2014.3.1411/js/kendo.all.min.js"></script>
    <script src="http://cdn.kendostatic.com/2014.3.1411/js/kendo.aspnetmvc.min.js"></script>

    <script src="~/Scripts/admintheme/jquery.uniform.min.js" type="text/javascript"></script>
    <script src="~/Scripts/admintheme/jquery-ui.js" type="text/javascript"></script>
    <script src="~/Scripts/admintheme/jquery.autosize.js" type="text/javascript"></script>
    <script src="~/Scripts/admintheme/jquery.scrollTo.js" type="text/javascript"></script>
    
  

</head>
<body class="ps_back-office page-sidebar admindashboard" id="a">
    
    @Html.Partial("_PartialHeader")
    
    <!-- begin main -->
    <div id="main">
        <!-- begin pusher -->
        <div class="pusher bootstrap">
            <!-- begin nav-sidebar -->
            @Html.Partial("_PartialNavSidebar")
            <!-- end nav-sidebar -->
        </div>
        <!-- end pusher -->
        <!-- begin content -->
        <div id="content" class="bootstrap">
            @Html.Partial("_PartialBreadCrumb")
            <div class="bootstrap">
                @RenderBody()
                <!-- begin modal -->
                @Html.Partial("_PartialModal")
                <!-- end modal -->
            </div>
        </div>
        <!-- end content -->
    </div>
    
   

    <!-- end main -->
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("javaScript", required: false)
    
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/admintheme/enquire.min.js" type="text/javascript"></script>
    <script src="~/Scripts/admintheme/admin-theme.js" type="text/javascript"></script>
</body>
</html>
